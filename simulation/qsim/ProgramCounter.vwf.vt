`timescale 1 ps/ 1 ps
module ProgramCounter_vlg_sample_tst(
	Clock,
	Incremente,
	PC_in,
	ResetSystem,
	SavePC,
	sampler_tx
);
input  Clock;
input  Incremente;
input [15:0] PC_in;
input  ResetSystem;
input  SavePC;
output sampler_tx;

reg sample;
time current_time;
always @(Clock or Incremente or PC_in or ResetSystem or SavePC)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

