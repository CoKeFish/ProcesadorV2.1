module ProgramCounter_vlg_vec_tst();
// constants                                           
// general purpose registers
reg Clock;
reg Incremente;
reg [15:0] PC_in;
reg ResetSystem;
reg SavePC;
// wires                                               
wire [15:0] PC_out;

wire sampler;                             

// assign statements (if any)                          
ProgramCounter i1 (
// port map - connection between master ports and signals/registers   
	.Clock(Clock),
	.Incremente(Incremente),
	.PC_in(PC_in),
	.PC_out(PC_out),
	.ResetSystem(ResetSystem),
	.SavePC(SavePC)
);

// Clock
always
begin
	Clock = 1'b0;
	Clock = #5000 1'b1;
	#5000;
end 

// ResetSystem
initial
begin
	ResetSystem = 1'b1;
end 

